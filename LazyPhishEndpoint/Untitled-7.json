{
    "manifest_version": 2,
    "name": "Access Control Extension",
    "version": "1.0",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "https://*/*",
      "http://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  